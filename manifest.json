{
	"manifest_version": 2,
	"name": "bgTabOpnr",
	"version": "0.1.0",
	"description": "Chrome extenstion opening liks matching specific rules in background tabs",
	"author": "alevsabdev",
	"browser_action": {
		"default_title": "Use this to open the popup",
		"default_popup": "popup/popup.html"
	},
	"background": {
		"scripts": ["background.js"]
	//	"persistent": false
	},
	  "content_scripts": [
	    {
	      "matches": ["https://www.datanyze.com/admin/?tab=tech_requests&tr_id=*"],
	      "css": ["checker/checker.css"],
	      "js": ["checker/checker.js"]
	    }
	  ],
	"permissions": ["tabs", "*://*/*", "http://techlookup.herokuapp.com/"]
}